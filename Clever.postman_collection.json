				{
					"name": "Recursos - Posts",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 201\", function (){\r",
									"    var expectResponseCode = pm.variables.get(\"statusCode\")\r",
									"    pm.response.to.have.status(expectResponseCode);\r",
									"});\r",
									"\r",
									"pm.test(\"Attribute id is 101\", function (){\r",
									"    jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.id).to.eql(101);\r",
									"});\r",
									"const postsSchema = {\r",
									"   userId : '',\r",
									"   title: '',\r",
									"   body: ''\r",
									"}\r",
									"\r",
									"const body = JSON.parse(pm.request.body.raw);\r",
									"const sameSchema = JSON.stringify(Object.keys(postsSchema)) === JSON.stringify(Object.keys(body));\r",
									"\r",
									"pm.test(\"Same schema\", () => {\r",
									"    pm.expect(true).to.be.eql(sameSchema);\r",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n   \"userId\":{{userId}},\r\n   \"title\":\"{{title}}\",\r\n   \"body\":\"{{body}}\"\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseurl}}/posts",
							"host": [
								"{{baseurl}}"
							],
							"path": [
								"posts"
							]
						}
					},
					"response": []
				},
				{
