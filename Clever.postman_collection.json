			"item": [
				{
					"name": "Recursos - Posts",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json();\r",
									"\r",
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Validate array length is 100\", function () {\r",
									"    pm.expect(jsonData).have.lengthOf(100);\r",
									"});\r",
									"\r",
									"pm.test(\"Validate quantity posts\", function (){\r",
									"    pm.expect(jsonData).not.equal(99);\r",
									"});\r",
									"\r",
									"pm.test(\"Validate user properties\", function () {\r",
									"    pm.expect(jsonData[0].userId).to.be.eql(1);\r",
									"    pm.expect(jsonData[0].id).to.be.eql(1);\r",
									"    pm.expect(jsonData[0].title).to.be.contain(\"sunt aut facere repellat provident occaecati excepturi optio reprehenderit\");\r",
									"    pm.expect(jsonData[0].body).to.include(\"sunt\");\r",
									"});\r",
									"\r",
									"pm.test(\"Validate that the property exists\", function (){\r",
									"    pm.expect(jsonData[0]).to.have.property(\"userId\");\r",
									"    pm.expect(jsonData[0]).to.have.property(\"id\");\r",
									"    pm.expect(jsonData[0]).to.have.property(\"title\");\r",
									"    pm.expect(jsonData[0]).to.have.property(\"body\");\r",
									"});\r",
									"\r",
									"pm.test(\"Validate the field type\", function (){\r",
									"    pm.expect(jsonData[0].userId).to.be.a(\"number\");\r",
									"    pm.expect(jsonData[0].id).to.be.a(\"number\");\r",
									"    pm.expect(jsonData[0].title).to.be.a(\"string\");\r",
									"    pm.expect(jsonData[0].body).to.be.a(\"string\");\r",
									"});\r",
									"\r",
									"pm.test(\"Validate that the object does not exist\", function (){\r",
									"    pm.expect(jsonData).to.not.have.any.keys(\"noExiste\");\r",
									"});\r",
									"\r",
									"pm.test(\"Response less than  200ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Validate that header exist\", function () {\r",
									"    pm.response.to.have.header(\"Content-Type\");\r",
									"    pm.response.to.have.header(\"Connection\");\r",
									"});\r",
									"\r",
									"pm.test(\"Content-Type header is application/json\", () => {\r",
									"  pm.expect(pm.response.headers.get('Content-Type')).to.eql('application/json; charset=utf-8');\r",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{baseurl}}/posts",
							"host": [
								"{{baseurl}}"
							],
