									"    pm.expect(jsonData[0].id).to.be.eql(1);\r",
									"    pm.expect(jsonData[0].url).to.be.contain(\"https://via.placeholder.com/600/92c952\");\r",
									"    pm.expect(jsonData[0].thumbnailUrl).to.include(\"https\");\r",
									"});\r",
									"\r",
									"pm.test(\"Validate that the property exists\", function (){\r",
									"    pm.expect(jsonData[0]).to.have.property(\"albumId\");\r",
									"    pm.expect(jsonData[0]).to.have.property(\"id\");\r",
									"    pm.expect(jsonData[0]).to.have.property(\"title\");\r",
									"    pm.expect(jsonData[0]).to.have.property(\"url\");\r",
									"    pm.expect(jsonData[0]).to.have.property(\"thumbnailUrl\");\r",
									"});\r",
									"\r",
